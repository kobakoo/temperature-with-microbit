{
    "name": "datalogger",
    "description": "Data logging to flash memory. micro:bit (V2) only.",
    "dependencies": {
        "core": "*",
        "flashlog": "*"
    },
    "files": [
        "datalogger.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "6.1.8",
        "pxt": "9.3.7"
    },
    "disablesVariants": [
        "mbdal"
    ],
    "yotta": {
        "config": {
            "MICROBIT_BLE_UTILITY_SERVICE": 1,
            "MICROBIT_BLE_UTILITY_SERVICE_PAIRING": 1
        }
    },
    "icon": "./static/libs/datalogger.png"
}
